/* ---------- new x2 2005 ---------------------------------------- */
item(FEAT_TRAINS, X2_2005, 0x91) {
    property {
        name:                 string(STR_X2_2005_NAME, 0);
        climates_available:   ALL_CLIMATES;
        introduction_date:    date(2005, 1, 1);
		model_life: 		  50;
        vehicle_life:         25;
        reliability_decay:    18;

        /* performance */
        power:                3817 kW;      // 5120 hp, +10 %
        speed:                242 km/h;     // 220 km/h + 10 %
        weight:               80;         	// 73 t + 10 %
		tractive_effort_coefficient: 0.34;
        air_drag_coefficient:  0;           // use default for this speed

        /* economy */
        cost_factor:          130;          // +30 % over base
		running_cost_base:    RUNNING_COST_ELECTRIC;
        running_cost_factor:  175;

        /* classification */
        engine_class:         ENGINE_CLASS_ELECTRIC;
        sprite_id:            SPRITE_ID_NEW_TRAIN;
        track_type:           _X5KV;

        /* bells & whistles */
        misc_flags: bitmask(TRAIN_FLAG_TILT, TRAIN_FLAG_MU);
    }

    graphics {
        default:  spr_x2_2005;
        purchase: spr_x2_2005_purchase;
    }
}

